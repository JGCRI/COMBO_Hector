esmname <- 'rgr2p5-CanESM5_ssp245'
esmfilelist <- list.files(paste0('cmip6_netcdf/', substr(esmname, 8, nchar(esmname))), full.names = TRUE)
esmfilelist <- esmfilelist[grepl('rgr2p5-', esmfilelist)]
latvar <- 'lat'
lonvar <- 'lon'
deltaTOS_refyear <- 2000
